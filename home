<script>
$('.carousel').carousel({
auto: false,
effect:"fade",
//height:250,
period: 1800,
duration: 3300,
markers: {
type: "cycle",
position:"top-right"
}
});



$("img").on('click', function(){
//var a='<div class="carousel">[0]</div>'.replace('[0]',$(this).children(".carousel").clone().text());
var a=$(this).clone();

	$.Dialog({
	overlay: true,
	shadow: true,
	flat: false,
	height:100,
	width:100,
	icon: '<span class=" icon-zoom-in"></span>',
	title: 'Zoom',
	padding: 10,
	//content:a
	onShow: function(_dialog){
	var content = _dialog.children('.content');
	content.append(a);	
	 
	}
	});
});


</script>
